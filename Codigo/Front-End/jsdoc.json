{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "src/components/",
      "src/pages/",
      "src/context/",
      "src/Tools/",
      "src/App.jsx",
      "src/main.jsx"
    ],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "node_modules/jsdoc-babel"],
  "opts": {
    "encoding": "utf8",
    "destination": "./Documentaci√≥n/",
    "recurse": true,
    "verbose": true
  }
}
